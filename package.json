{
  "name": "ibay_apartment_hunter",
  "version": "1.0.0",
  "description": "Telegram Bot to fetch the latest apartment listings from iBay and other sources in Maldives",
  "main": "build/app.js",
  "type": "module",
  "homepage": "https://github.com/ayarse/ibay_apartment_hunter",
  "bugs": {
    "url": "https://github.com/ayarse/ibay_apartment_hunter/issues"
  },
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts  --inspect",
    "build": "tsc",
    "clean": "rm -r ./build",
    "drizzle:push": "drizzle-kit push",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "lint": "biome format --write",
    "script:add-structured-data": "tsx ./scripts/add-structured-data-to-existing.ts"
  },
  "author": "ayarse",
  "license": "MIT",
  "dependencies": {
    "@minify-html/node": "^0.16.4",
    "@sentry/node": "^10.5.0",
    "crawlee": "^3.14.1",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "env-cmd": "^10.1.0",
    "grammy": "^1.38.1",
    "grammy-inline-menu": "^9.2.0",
    "openai": "^5.16.0",
    "p-queue": "^9.0.0",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "znv": "^0.5.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
