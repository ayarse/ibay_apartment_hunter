{
  "name": "ibay_apartment_hunter",
  "version": "1.0.0",
  "description": "Telegram Bot to fetch the latest apartment listings from iBay and other sources in Maldives",
  "main": "build/app.js",
  "type": "module",
  "homepage": "https://github.com/ayarse/ibay_apartment_hunter",
  "bugs": {
    "url": "https://github.com/ayarse/ibay_apartment_hunter/issues"
  },
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts  --inspect",
    "build": "tsc",
    "clean": "rm -r ./build",
    "generate": "drizzle-kit generate",
    "migrate": "env-cmd -f ./.env tsx scripts/migrate.ts",
    "push": "drizzle-kit push:pg",
    "dk:migrate": "drizzle-kit migrate",
    "lint": "biome format --write"
  },
  "author": "ayarse",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.86.0",
    "crawlee": "^3.12.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.43.1",
    "env-cmd": "^10.1.0",
    "grammy": "^1.19.2",
    "grammy-inline-menu": "^8.0.1",
    "pino": "^8.16.2",
    "postgres": "^3.4.5",
    "tsx": "^4.6.2",
    "znv": "^0.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@types/node": "^20.10.4",
    "drizzle-kit": "^0.31.0",
    "pino-pretty": "^13.1.1",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
